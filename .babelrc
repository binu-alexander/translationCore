{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "react"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    "transform-decorators-legacy",
    "transform-async-to-generator",
    [
      "transform-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ],
    "jest-hoist"
  ],
  "ignore": [
    "node_modules"
  ]
}
